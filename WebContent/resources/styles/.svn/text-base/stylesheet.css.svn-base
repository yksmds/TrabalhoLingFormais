/*Classes utilizadas como base para montagem do panel-grid*/




/* Estilo utilizado para os componentes em HTML  */








.input-text {
	font-family: verdana, serif, sans-serif;
	font-size: 12px;
	padding: 1px;
	border-color: #999999;
	border-width: 1px;
	border-style: solid;
	height: 20px;
	
}

.input-text-bold {
	font-family: verdana, serif, sans-serif;
	font-size: 12px;
	height: 20px;
	font-weight: bold;
	
}

.input-text-radius {
	font-family: verdana, serif, sans-serif;
	font-size: 12px;
	padding: 1px;
	height: 20px;
	border-radius: 5px;
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px;"
}



.list-text {
	font-family: verdana, serif, sans-serif;
	font-size: 12px;
	padding: 1px;
	border-color:#999999;
	border-width: 1px;
	border-style: solid;
	background: #FFFFFF;
}



.textUsuario {
	font-family: verdana, serif, sans-serif;
	font-size: 10px;
	color:  #333333;
	position: absolute;
	left: 85px;
	top: 65px;
}

.command-button {
	font-family: verdana, serif, sans-serif;
	height: 20px;
	font-size: 11px;
	color: #333333;
	background-color: #D9D9D9;
	border-color: #333333;
	border-width: 1px;
	border-style: solid;
}

.botaoProcedure {
	border-width: 1px;
	border-style: solid;
	border-color: #999999;
	font-family: verdana, serif, sans-serif;
	font-size: 11px;
	color: #999999;
	background-color: #FFFFFF;
}


.component-panel {
	border: 1px solid #999999;
}







/* Links */
a:link {
	vertical-align: top;
	font-family: verdana, serif, sans-serif;
	color:  #333333;
	font-size: 11px;
	text-decoration: underline;
}

a:visited {
	vertical-align: top;
	font-family: verdana, serif, sans-serif;
	color:  #333333;
	font-size: 11px;
	text-decoration: underline;
}


 



