body {margin:0;}

#header {
		width:100%;
		background:#003c7b;							
		background-image: url('../images/header-wrapper-bg.jpg');
		padding: 20px;
		overflow:hidden;
		font-size:12px;
		font-family:sans-serif;
}
#logo
{
		width:250px;
		height:50px;
		float:left;
		margin:10px 0 0 10px;
}

#usuario {
		float: right;
		width: 200px;
		height: 30px;
		padding-left: 30px;
		margin-top:10px;
		background: url('../images/user.png') no-repeat;
							
}
#usuario span
{
		position: relative;
		top:5px;
		color: #fff;
		font-weight: bold;
}








/* Style rules to make data tables look better */




/*Style rules for message severity levels */
.infoMessage {
	border-width: 1px;
	border-style: solid;
	border-color: #999999;
	font-family: verdana, serif, sans-serif;
	font-size: 11px;
	color:#333333;
	background-color: #FEC194;
	padding: 3px;
	text-align: center;
	vertical-align: middle;
}

.warnMessage {
	border-width: 1px;
	border-style: solid;
	border-color: #999999;
	font-family: verdana, serif, sans-serif;
	font-size: 11px;
	background-color: #99CCFF;
	padding: 3px;
	text-align: center;
	vertical-align: middle;
}

.errorMessage {
	border-width: 1px;
	border-style: solid;
	border-color: #999999;
	font-family: verdana, serif, sans-serif;
	font-size: 11px;
	color: #FFFFFF;
	background-color: #C06C6C;
	padding: 3px;
	text-align: center;
	vertical-align: middle;
}

.fatalMessage {
	border-width: 1px;
	border-style: solid;
	border-color: #999999;
	font-family: verdana, serif, sans-serif;
	font-size: 11px;
	color: #FFFFFF;
	background-color: #EF1408;
	padding: 3px;
	text-align: center;
	vertical-align: middle;
}

.helpbuttom {
	position: absolute;
	left: 30px;
	top: 550px;
	
}







